<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BBE1FA"
    tools:context=".TaskActivityDay5">

    <CheckBox
        android:id="@+id/box51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="36dp"
        android:text="CheckBox"
        app:layout_constraintBottom_toTopOf="@+id/box52"
        app:layout_constraintEnd_toEndOf="@+id/box52" />

    <CheckBox
        android:id="@+id/box52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="28dp"
        android:text="CheckBox"
        app:layout_constraintBottom_toTopOf="@+id/box53"
        app:layout_constraintEnd_toEndOf="parent" />

    <CheckBox
        android:id="@+id/box53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:text="CheckBox"
        app:layout_constraintBottom_toTopOf="@+id/box54"
        app:layout_constraintStart_toStartOf="@+id/box52" />

    <CheckBox
        android:id="@+id/box54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:text="CheckBox"
        app:layout_constraintBottom_toTopOf="@+id/box55"
        app:layout_constraintStart_toStartOf="@+id/box53" />

    <CheckBox
        android:id="@+id/box55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:text="CheckBox"
        app:layout_constraintBottom_toTopOf="@+id/save5"
        app:layout_constraintStart_toStartOf="@+id/box54" />

    <ProgressBar
        android:id="@+id/pb51"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="167dp"
        android:layout_height="32dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toTopOf="@+id/box51"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/save5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="97dp"
        android:text="save"
        android:background="#3282B8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>